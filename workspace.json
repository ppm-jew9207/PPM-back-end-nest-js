{
  "version": 1,
  "projects": {
    "ppm": {
      "root": "apps/ppm",
      "sourceRoot": "apps/ppm/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/ppm",
            "index": "apps/ppm/src/index.html",
            "main": "apps/ppm/src/main.tsx",
            "polyfills": "apps/ppm/src/polyfills.ts",
            "tsConfig": "apps/ppm/tsconfig.app.json",
            "assets": ["apps/ppm/src/favicon.ico", "apps/ppm/src/assets"],
            "styles": ["apps/ppm/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/ppm/src/environments/environment.ts",
                "with": "apps/ppm/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ppm:build",
            "proxyConfig": "apps/ppm/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "ppm:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/ppm/tsconfig.app.json",
              "apps/ppm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ppm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ppm/jest.config.js",
            "tsConfig": "apps/ppm/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
